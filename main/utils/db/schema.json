{
  "settings": {
    "server_id": "BIGINT PRIMARY KEY NOT NULL,",
    "owner_id": "BIGINT NOT NULL,",
    "prefix": "TEXT DEFAULT NULL,",
    "logging_chn": "BIGINT DEFAULT NULL,",
    "exceptions": "BIGINT DEFAULT NULL,",
    "hashtags": "BIGINT ARRAY DEFAULT NULL,",
    "levels": "BOOLEAN DEFAULT NULL," ,
    "rep": "BOOLEAN DEFAULT NULL,",
    "playchns": "BOOLEAN DEFAULT NULL,",
    "playcat": "BIGINT DEFAULT NULL,",
    "setup": "BOOLEAN NOT NULL DEFAULT FALSE"
  },

  "logger": {
    "server_id": "BIGINT NOT NULL,",
    "user_id": "BIGINT NOT NULL,",
    "channel_id": "BIGINT NOT NULL,",
    "last_msg": "TIMESTAMP DEFAULT CURRENT_DATE,",
    "last_rep_given": "TIMESTAMP,",
    "msg_count": "BIGINT NOT NULL,",
    "PRIMARY KEY": "(server_id, user_id)" 
  },

  "playchns": {
    "server_id": "BIGINT NOT NULL,",
    "user_id": "BIGINT NOT NULL,",
    "count": "INT NOT NULL,", 
    "chns": "BIGINT ARRAY DEFAULT NULL,",   
    "PRIMARY KEY": "(server_id, user_id)" 
  },

  "quests": {
    "server_id": "BIGINT NOT NULL,",
    "user_id": "BIGINT NOT NULL,",
    "quest_id": "INT NOT NULL,",
    "type": "TEXT,",
    "posted_on": "TIMESTAMP DEFAULT CURRENT_DATE,",
    "msg": "TEXT,",
    "PRIMARY KEY": "(server_id, user_id)" 
  },

  "rep": {
    "server_id": "BIGINT NOT NULL,",
    "user_id": "BIGINT NOT NULL,",
    "rep": "INT NOT NULL,",
    "last_given": "TIMESTAMP DEFAULT CURRENT_DATE,",
    "CONSTRAINT": "server_user UNIQUE (server_id, user_id)"
  },

  "rewards": {
    "server_id": "BIGINT NOT NULL,",
    "role_id": "BIGINT NOT NULL,",
    "type": "TEXT NOT NULL,",
    "val": "INTEGER NOT NULL,",
    "PRIMARY KEY": "(server_id, role_id, type)"
  },

  "tags": {
    "id": "VARCHAR(28) PRIMARY KEY,",
    "server_id": "BIGINT NOT NULL,",
    "user_id":"BIGINT NOT NULL,",
    "name": "TEXT NOT NULL,",
    "description": "TEXT NOT NULL"
  },

  "threads": {
    "server_id": "BIGINT PRIMARY KEY NOT NULL,",
    "channels": "BIGINT ARRAY,",
    "threads": "BIGINT ARRAY "
  },

  "xp": {
    "server_id": "BIGINT NOT NULL,",
    "user_id": "BIGINT NOT NULL,",
    "xp": "INT NOT NULL,",
    "level": "INT NOT NULL,",
    "last_xp": "TIMESTAMP DEFAULT CURRENT_DATE,",
    "PRIMARY KEY": " (server_id, user_id)" 
  }
}