{
  "settings": {
    "server_id": "BIGINT PRIMARY KEY NOT NULL,",
    "owner_id": "BIGINT NOT NULL,",
    "prefix": "TEXT DEFAULT NULL,",
    "loggingChn": "BIGINT"
  },

  "logger": {
    "serverId": "BIGINT PRIMARY KEY NOT NULL,",
    "userId": "BIGINT NOT NULL,",
    "channelId": "BIGINT NOT NULL,",
    "lastMsg": "DATE NOT NULL"
  },

  "quests": {
    "serverId": "BIGINT PRIMARY KEY NOT NULL,",
    "userId": "BIGINT NOT NULL,",
    "questID": "INT NOT NULL,",
    "type": "TEXT,",
    "postedOn": "DATE DEFAULT CURRENT_DATE,",
    "msg": "TEXT"
  },

  "playchns": {
    "serverId": "BIGINT PRIMARY KEY NOT NULL,",
    "userId": "BIGINT NOT NULL,",
    "map": "BOOLEAN DEFAULT FALSE"   
  },

  "rep": {
    "serverId": "BIGINT PRIMARY KEY NOT NULL,",
    "userId": "BIGINT NOT NULL,",
    "rep": "INT NOT NULL,",
    "lastGiven": "DATE DEFAULT CURRENT_DATE,",
    "CONSTRAINT": "server_user UNIQUE (serverid, userid));"
  },

  "xp": {
    "serverId": "BIGINT PRIMARY KEY NOT NULL,",
    "userId": "BIGINT NOT NULL,",
    "xp": "INT NOT NULL,",
    "level": "INT NOT NULL"
  }
}